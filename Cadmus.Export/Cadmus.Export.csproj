<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Version>2.0.4</Version>
		<FileVersion>2.0.4</FileVersion>
		<AssemblyVersion>2.0.4</AssemblyVersion>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<Authors>Daniele Fusi</Authors>
		<Company>Fusi</Company>
		<Product>Cadmus</Product>
		<Description>Cadmus export components.</Description>
		<Copyright>by Daniele Fusi 2022</Copyright>
		<NeutralLanguage>en-US</NeutralLanguage>
		<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
		<PackageTags>Cadmus</PackageTags>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Assets\Iso639.txt" />
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="Assets\Iso639.txt" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Cadmus.Core" Version="6.0.3" />
		<PackageReference Include="Cadmus.General.Parts" Version="4.2.1" />
		<PackageReference Include="Cadmus.Mongo" Version="6.0.3" />
		<PackageReference Include="Fusi.Microsoft.Extensions.Configuration.InMemoryJson" Version="3.0.0" />
		<PackageReference Include="Fusi.Tools.Configuration" Version="3.1.1" />
		<PackageReference Include="Fusi.Xml" Version="3.0.1" />
		<PackageReference Include="Fusi.Xml.Extras" Version="3.0.1" />
		<PackageReference Include="JmesPath.Net" Version="1.0.308" />
		<PackageReference Include="Markdig" Version="0.31.0" />
	</ItemGroup>

</Project>
